		$(".myjob").click(function(){
				 $.ajax({
			 type:"POST",
			 url:ctxLocal+'semester/getSemesterData.action',
			 data:{userId:loginId},
			 dataType:"json",
			 async:false,
			 success: function(res) {
								if(res.code==0){
